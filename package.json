        "fp-ts": "^2.16.0",
        "fs-extra": "^11.1.1",
        "io-ts": "^2.2.20",
        "tslib": "^2.6.0",
        "yaml": "^2.3.1",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.4.2",
        "@types/yargs": "^17.0.24",
        "autorest": "^3.6.3",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
